                          {% endif %}
                          {% if tenderRequirement.dataType == "number" or tenderRequirement.dataType == "integer" %}
                            {% if tenderRequirement.expectedValue != null %}
                              ,
                              "expectedValue": {{ tenderRequirement.expectedValue }}
                            {% endif %}
                            {% if tenderRequirement.minValue != null %}
                              ,
                              "minValue": {{ tenderRequirement.minValue }}
                            {% endif %}
                            {% if tenderRequirement.maxValue != null %}
                              ,
                              "maxValue": {{ tenderRequirement.maxValue }}
                            {% endif %}
                            {% if tenderRequirement.conversions != null %}
                              ,
                              "conversions": [
                                {% for tenderConversion in tenderRequirement.conversions %}
                                  {% if loop.index  > 0 %},{% endif %}
                                  {
                                    "id": "{{ tenderConversion.id }}",
                                    "relatesTo": "{{ tenderConversion.relatesTo }}",
                                    "relatedItem": "{{ tenderConversion.relatedItem }}",
                                    {{ stringAttributeIf(name = "description", value = tenderConversion.description) }}
                                    {{ commaIf(tenderConversion.description) }}
                                    "rationale": "{{ tenderConversion.rationale }}",
                                    "coefficients": [
                                      {% for tenderCoefficient in tenderConversion.coefficients %}
                                        {% if loop.index  > 0 %},{% endif %}
                                        {
                                          "id": "{{ tenderCoefficient.id }}",
                                          "value": {{ tenderCoefficient.value }},
                                          "coefficient": {{ tenderCoefficient.coefficient }}
                                        }
                                      {% endfor %}
                                    ]
                                  }
                                {% endfor %}
                              ]
                            {% endif %}
                          {% endif %}
                          {% if tenderRequirement.dataType == "boolean" and tenderRequirement.expectedValue != null %}
                            ,
                            "expectedValue": {{ tenderRequirement.expectedValue }}
                            {% if tenderRequirement.conversions != null %}
                              ,
                              "conversions": [
                                {% for tenderConversion in tenderRequirement.conversions %}
                                  {% if loop.index  > 0 %},{% endif %}
                                  {
                                    "id": "{{ tenderConversion.id }}",
                                    "relatesTo": "{{ tenderConversion.relatesTo }}",
                                    "relatedItem": "{{ tenderConversion.relatedItem }}",
                                    {{ stringAttributeIf(name = "description", value = tenderConversion.description) }}
                                    {{ commaIf(tenderConversion.description) }}
                                    "rationale": "{{ tenderConversion.rationale }}",
                                    "coefficients": [
                                      {% for tenderCoefficient in tenderConversion.coefficients %}
                                        {% if loop.index  > 0 %},{% endif %}
                                        {
                                          "id": "{{ tenderCoefficient.id }}",
                                          "value": {{ tenderCoefficient.value }},
                                          "coefficient": {{ tenderCoefficient.coefficient }}
                                        }
                                      {% endfor %}
                                    ]
                                  }
                                {% endfor %}
                              ]
                            {% endif %}
                          {% endif %}
                        }
                      {% endfor %}
                    ]
                  }
                {% endfor %}
              ]
            }
          {% endfor %}
        ]
      {% endif %}
      {% if context.tender.procurementMethodModalities != null %}
        ,
        "modalities": {
          "procurementMethodModalities": ["{{ context.tender.procurementMethodModalities }}"],
          "details": [
            {% for electronicAuction in context.tender.electronicAuctions %}
              {% if loop.index  > 0 %},{% endif %}
              {
                "id": "{{ electronicAuction.id }}",
                "relatedLot": "{{ electronicAuction.relatedLot }}",
                "electronicAuctionModalities": [
                  {
                    "amount": "{{ electronicAuction.value.amount }}",
                    "currency": "{{ electronicAuction.value.currency }}"
                  }
                ]
              }
            {% endfor %}
          ]
        }
      {% endif %}
    },
    "section4": {
      "method": {
        "procurementMethodDetails": "{{ context.tender.procurementMethodDetails }}",
        "legalBasis": "{{ context.tender.legalBasis }}"
        {{ commaIf(context.tender.awardCriteria) }}
        {{ stringAttributeIf(name = "awardCriteria", value = context.tender.awardCriteria) }}
        {{ commaIf(context.tender.awardCriteriaDetails) }}
        {{ stringAttributeIf(name = "awardCriteriaDetails", value = context.tender.awardCriteriaDetails) }}
      }
      {% if context.tender.enquiryPeriod != null and context.tender.tenderPeriod != null %}
        ,
        "administrative": {
          "enquiryPeriod": {
            "endDate": "{{ context.tender.enquiryPeriod }}"
          },
          "tenderPeriod": {
            "endDate": "{{ context.tender.tenderPeriod }}"
          }
        }
      {% endif %}
    },
    "budget": {
      {% if context.tender.budgetBreakdown != null %}
        "budgetBreakdown": [
          {% for budgetBreakdown in context.tender.budgetBreakdown %}
            {% if loop.index  > 0 %},{% endif %}
            {
              "id": "{{ budgetBreakdown.id }}",
              "budgetBreakdownAmount": {
                "amount": {{ budgetBreakdown.amount.amount }},
                "currency": "{{ budgetBreakdown.amount.currency }}"
              }
            }
          {% endfor %}
        ]
      {% endif %}
    }
  }
}
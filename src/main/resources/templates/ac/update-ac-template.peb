              "amountNet": {
                "title": "",
                "type": "number",
                "ocds": "award::object/value::object/amountNet::number"
              },
              "valueAddedTaxIncluded": {
                "title": "",
                "type": "boolean",
                "ocds": "award::object/value::object/valueAddedTaxIncluded::boolean"
              },
              "currency": {
                "title": "",
                "type": "string",
                "default": "{{ context.currency }}",
                "readOnly": true,
                "ocds": "award::object/value::object/currency::string"
              }
            }
          },
          "documents": {
            "title": "",
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "object",
              "required": [
                "documentId",
                "documentType"
              ],
              "properties": {
                "file": {
                  "title": "Select file",
                  "readOnly": true,
                  "type": "string"
                },
                "documentId": {
                  "type": "string",
                  "readOnly": true
                },
                "title": {
                  "title": "Document title",
                  "type": "string",
                  "readOnly": true
                },
                "description": {
                  "title": "Brief description for this document",
                  "type": "string",
                  "readOnly": true
                },
                "documentType": {
                  "title": "Document type",
                  "type": "string",
                  "readOnly": true,
                  "enum": [
                    ""
                  ],
                  "enumNames": [
                    ""
                  ]
                },
                "relatedLots": {
                  "type": "array",
                  "minItems": 1,
                  "maxItems": 1,
                  "items": {
                    "type": "string",
                    "default": "{{ context.lotId }}",
                    "readOnly": true
                  }
                }
              }
            }
          }
        }

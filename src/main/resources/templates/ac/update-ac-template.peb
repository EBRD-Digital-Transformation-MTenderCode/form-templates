                              "enum": [
                                "from standarized list",
                                "by manual input"
                              ]
                            }
                          },
                          "dependencies": {
                            "localityType": {
                              "oneOf": [
                                {
                                  "properties": {
                                    "localityType": {
                                      "enum": [
                                        "from standarized list"
                                      ]
                                    },
                                    "directory": {
                                      "type": "object",
                                      "properties": {
                                        "locality": {
                                          "title": "Town/Locality",
                                          "type": "object",
                                          "source": "{{ context.itemUris.locality }}",
                                          "dependentFields": {
                                            "region": "award.items[itemIndex].deliveryAddress.location.region.id"
                                          },
                                          "properties": {
                                            "scheme": {
                                              "type": "string",
                                              "const": "CUATM",
                                              "ocds": "award::object/items::array[object]/deliveryAddress::object/addressDetails::object/locality::object/scheme::string"
                                            },
                                            "id": {
                                              "type": "string",
                                              "ocds": "award::object/items::array[object]/deliveryAddress::object/addressDetails::object/locality::object/id::string"
                                            },
                                            "description": {
                                              "type": "string",
                                              "ocds": "award::object/items::array[object]/deliveryAddress::object/addressDetails::object/locality::object/description::string"
                                            }

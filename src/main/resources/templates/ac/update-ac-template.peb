  "schema": {
    "type": "object",
    "properties": {
      "contract": {
        "type": "object",
        "title": " Contract Summary",
        "properties": {
          "title": {
            "title": "Title",
            "description": "Contract subject/scope of work",
            "type": "string",
            "ocds": "contract::object/title::string"
          },
          "description": {
            "title": "Short description",
            "description": "Brief description of the subject of the contract",
            "type": "string",
            "ocds": "contract::object/descripiton::string"
          },
          "period": {
            "title": "",
            "type": "object",
            "required": [
              "startDate",
              "endDate"
            ],
            "properties": {
              "startDate": {
                "title": "",
                "type": "string",
                "ocds": "contract::object/period::object/startDate::string"
              },
              "endDate": {
                "title": "",
                "type": "string",
                "ocds": "contract::object/period::object/endDate::string"
              }
            }
          },
          "documents": {
            "title": "Attachments and documents of Contract",
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "documentId",
                "documentType"
              ],
              "properties": {
                "file": {
                  "title": "Select file",
                  "type": "string"
                },
                "documentId": {
                  "type": "string",
                  "ocds": "contract::object/documents::array[object]/id::string"
                },
                "title": {
                  "title": "Document title",
                  "type": "string",
                  "ocds": "contract::object/documents::array[object]/title::string"
                },
                "description": {
                  "title": "Brief description for this document",
                  "type": "string",
                  "ocds": "contract::object/documents::array[object]/description::string"
                },
                "documentType": {
                  "title": "Document type",
                  "type": "string",
                  "enum": [
                    ""
                  ],
                  "enumNames": [
                    ""
                  ],
                  "ocds": "contract::object/documents::array[object]/documentType::string"
                },
                "relatedLots": {
                  "type": "array",
                  "minItems": 1,
                  "maxItems": 1,
                  "items": {
                    "type": "string",
                    "default": "{{ context.lotId }}",
                    "ocds": "contract::object/documents::array[object]/relatedLots::array[string]"
                  }
                },
                "mustConfirmationRequests": {
                  "title": " Signatories required",
                  "type": "boolean",
                  "default": false
                }
              },
              "dependencies": {
                "mustConfirmationRequests": {
                  "oneOf": [
                    {
                      "properties": {
                        "mustConfirmationRequests": {
                          "enum": [
                            true
                          ]
                        },
                        "confirmationRequests": {
                          "type": "object",
                          "required": [
                            "id",
                            "relatedItem",
                            "source"
                          ],
                          "properties": {
                            "id": {
                              "type": "string",
                              "ocds": "contract::object/confirmationRequests::array[object]/id::string"
                            },
                            "relatedItem": {
                              "type": "string",
                              "ocds": "contract::object/confirmationRequests::array[object]/relatedItem::string"
                            },
                            "source": {
                              "type": "string",
                              "enum": [
                                "buyer",
                                "tenderer"
                              ],
                              "enumNames": [
                                "buyer",
                                "tenderer"
                              ],
                              "ocds": "contract::object/confirmationRequests::array[object]/source::string"
                            }
                          }
                        }
                      }
                    },
                    {
                      "properties": {
                        "mustConfirmationRequests": {
                          "enum": [
                            false
                          ]
                        }
                      }
                    }
                  ]
                }
              }
            }
          }
        }
      },

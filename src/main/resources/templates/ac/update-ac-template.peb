                                            "scheme": {
                                              "type": "string",
                                              "const": "other",
                                              "ocds": "award::object/items::array[object]/deliveryAddress::object/addressDetails::object/locality::object/scheme::string"
                                            },
                                            "id": {
                                              "type": "string",
                                              "const": "999999999",
                                              "ocds": "award::object/items::array[object]/deliveryAddress::object/addressDetails::object/locality::object/id::string"
                                            },
                                            "description": {
                                              "type": "string",
                                              "ocds": "award::object/items::array[object]/deliveryAddress::object/addressDetails::object/locality::object/description::string"
                                            }
                                          }
                                        }
                                      }
                                    }
                                  }
                                }
                              ]
                            }
                          }
                        }
                      }
                    },
                    "postalAddress": {
                      "title": "Postal Address",
                      "type": "object",
                      "required": [
                        "streetAddress"
                      ],
                      "properties": {
                        "streetAddress": {
                          "title": "Street Address",
                          "type": "string",
                          "ocds": "award::object/items::array[object]/deliveryAddress::object/streetAddress::string"
                        },
                        "postalCode": {
                          "title": "Postal Code",
                          "type": "string",
                          "ocds": "award::object/items::array[object]/deliveryAddress::object/postalCode::string"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "value": {
            "title": "",
            "type": "object",
            "required": [
              "amount",
              "amountNet",
              "valueAddedTaxIncluded",

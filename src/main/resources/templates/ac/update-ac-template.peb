        "minItem": 1,
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "title": "",
              "type": "string",
              "readOnly": true,
              "ocds": "contract::object/agreedMetrics::array[object]/id::string"
            },
            "title": {
              "title": "",
              "type": "sting",
              "readOnly": true
            },
            "description": {
              "title": "",
              "type": "sting",
              "readOnly": true
            },
            "observations": {
              "title": "",
              "type": "array",
              "minItem": 1,
              "items": {
                "type": "object",
                "properties": {
                  "id": {
                    "title": "",
                    "type": "string",
                    "readOnly": true,
                    "ocds": "contract::object/agreedMetrics::array[object]/observations::array[object]/id::string"
                  },
                  "notes": {
                    "title": "",
                    "readOnly": true,

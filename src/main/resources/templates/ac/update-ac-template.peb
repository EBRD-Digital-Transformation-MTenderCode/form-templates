                                      }
                                    }
                                  }
                                },
                                {
                                  "properties": {
                                    "localityType": {
                                      "enum": [
                                        "by manual input"
                                      ]
                                    },
                                    "manual": {
                                      "type": "object",
                                      "properties": {
                                        "locality": {
                                          "title": "Town/Locality",
                                          "type": "object",

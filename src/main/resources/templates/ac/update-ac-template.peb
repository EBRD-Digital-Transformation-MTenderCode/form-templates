                  "required": [
                    "amount",
                    "amountNet",
                    "valueAddedTaxIncluded",
                    "currency"
                  ],
                  "properties": {
                    "amount": {
                      "title": "",
                      "type": "number",
                      "ocds": "award::object/items::array[object]/unit::object/value::object/amount::number"
                    },
                    "amountNet": {
                      "title": "",
                      "type": "number",
                      "ocds": "award::object/items::array[object]/unit::object/value::object/amountNet::number"

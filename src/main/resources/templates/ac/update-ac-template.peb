                            "id": {
                              "type": "string",
                              "ocds": "award::object/items::array[object]/deliveryAddress::object/addressDetails::object/country::object/id::string"
                            },
                            "description": {
                              "type": "string",
                              "ocds": "award::object/items::array[object]/deliveryAddress::object/addressDetails::object/country::object/description::string"
                            }
                          }
                        },
                        "region": {
                          "title": "Region",
                          "type": "object",
                          "source": "{{ context.itemUris.region }}",
                          "properties": {
                            "id": {
                              "type": "string",
                              "ocds": "award::object/items::array[object]/deliveryAddress::object/addressDetails::object/region::object/id::string"
                            },
                            "description": {
                              "type": "string",
                              "ocds": "award::object/items::array[object]/deliveryAddress::object/addressDetails::object/region::object/description::string"
                            }
                          }
                        },
                        "locality": {
                          "type": "object",
                          "properties": {
                            "localityType": {
                              "title": "Specify town/locality",

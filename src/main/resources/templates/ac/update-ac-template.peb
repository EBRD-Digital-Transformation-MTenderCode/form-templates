                          "ocds": "planning::object/budget::object/budgetAllocation::array[object]/period::object/startDate::string"
                        },
                        "endDate": {
                          "title": "",
                          "type": "sting",
                          "ocds": "planning::object/budget::object/budgetAllocation::array[object]/period::object/endDate::string"
                        }
                      }
                    },
                    "amount": {
                      "title": "",
                      "type": "number",
                      "ocds": "planning::object/budget::object/budgetAllocation::array[object]/amount::number"
                    },
                    "relatedItem": {
                      "title": "",
                      "type": "string",
                      "ocds": "planning::object/budget::object/budgetAllocation::array[object]/relatedItem::string"
                    }
                  }
                }
              },
              "budgetSource": {
                "title": "",
                "type": "array",
                "items": {
                  "type": "object",
                  "required": [
                    "budgetBreakdownID",
                    "amount",
                    "currency"
                  ],
                  "properties": {
                    "budgetBreakdownID": {
                      "title": "",
                      "type": "object",
                      "ocds": "planning::object/budget::object/budgetSource::array[object]/budgetBreakdownID::string"
                    },
                    "amount": {
                      "title": "",
                      "type": "number",
                      "ocds": "planning::object/budget::object/budgetSource::array[object]/amount::number"
                    },
                    "currency": {
                      "title": "",
                      "type": "string",
                      "default": "{{ context.currency }}",
                      "readOnly": true,
                      "ocds": "planning::object/budget::object/budgetSource::array[object]/currency::string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "data": {
    "contract": {
      {{ stringAttributeIf(name = "title", value = context.contract.title.title) }}
      {{ commaIf(context.contract.description) }}
      {{ stringAttributeIf(name = "description", value = context.contract.description) }}
      "period": {
        {{ stringAttributeIf(name = "startDate", value = context.contract.period.startDate) }}
        {{ commaIf(context.contract.period.endDate) }}
        {{ stringAttributeIf(name = "endDate", value = context.contract.period.endDate) }}
      }
      {% if context.contract.documents != null %}
        ,
        "documents": [
          {% for contractDoc in context.contract.documents %}
            {% if loop.index  > 0 %},{% endif %}
            {
              "documentId": "{{ contractDoc.id }}",
              "documentType": "{{ contractDoc.type }}",
              "relatedLots": ["{{ contractDoc.relatedLots }}"],
              "mustConfirmationRequests": {% if contractDoc.confirmationRequest != null %}true{% else %}false{% endif %}
              {% if contractDoc.confirmationRequest != null %}
                ,
                "confirmationRequests": {
                  "id": "{{ contractDoc.confirmationRequest.id }}",
                  "relatedItem": "{{ contractDoc.confirmationRequest.relatedItem }}",
                  "source": "{{ contractDoc.confirmationRequest.source }}"
                }
              {% endif %}
              {{ commaIf(contractDoc.title) }}
              {{ stringAttributeIf(name = "title", value = contractDoc.title) }}
              {{ commaIf(contractDoc.description) }}
              {{ stringAttributeIf(name = "description", value = contractDoc.description) }}
            }
          {% endfor %}
        ]
      {% endif %}
    },
    "award": {
      "id": "{{ context.award.id }}",
      "items": [
        {% for item in context.award.items %}
          {% if loop.index  > 0 %},{% endif %}
          {
            "id": "{{ item.id }}",
            "quantity": {{ item.quantity }},
            {% if item.value != null %}
              "value": {
                "amount": {{ item.value.amount }},
                "amountNet": {{ item.value.amountNet }}
                "valueAddedTaxIncluded": {{ item.value.valueAddedTaxIncluded }},
                "currency": "{{ context.currency }}"
              }
            {% endif %}
            "deliveryAddress": {
              "location": {
                "country": {
                  "id": "{{ item.deliveryAddress.country.id }}",
                  "description": "{{ item.deliveryAddress.country.description }}"
                },
                "region": {
                  "id": "{{ item.deliveryAddress.region.id }}", 
                  "description": "{{ item.deliveryAddress.region.description }}"
                },
                "locality": {
                  "localityType": {% if item.deliveryAddress.locality.scheme == "CUATM" %}"{{ translate("cn", "section2.lots.performance.address.location.locality.localityType.enum[0]") }}"{% else %}"{{ translate("cn", "section2.lots.performance.address.location.locality.localityType.enum[1]") }}"{% endif %},
                    {% if item.deliveryAddress.locality.scheme == "CUATM" %}"directory"{% else %}"manual"{% endif %}: {
                      "locality": {
                        "scheme": "{{ item.deliveryAddress.locality.scheme }}",
                        "id": "{{ item.deliveryAddress.locality.id }}",
                        "description": "{{ item.deliveryAddress.locality.description }}"
                      }
                    }
                  }
                },
                "postalAddress": {
                "streetAddress": "{{ item.deliveryAddress.streetAddress }}"
                {{ commaIf(item.deliveryAddress.postalCode) }}
                {{ stringAttributeIf(name = "postalCode", value = item.deliveryAddress.postalCode) }}
              }
            }
          }
        {% endfor %}
      ],
      "value": {
        "amount": {{ context.award.value.amount }},
        "currency": "{{ context.currency }}"
        {{ commaIf(context.award.value.amountNet) }}
        {{ stringAttributeIf(name = "amountNet", value = context.award.value.amountNet) }}
        {{ commaIf(context.award.value.valueAddedTaxIncluded) }}
        {{ stringAttributeIf(name = "valueAddedTaxIncluded", value = context.award.value.valueAddedTaxIncluded) }}
      },
      "documents": [
        {% for awardDoc in context.award.documents %}
          {% if loop.index  > 0 %},{% endif %}
          {
            "documentId": "{{ awardDoc.id }}",
            "documentType": "{{ awardDoc.type }}",
            "relatedLots": ["{{ awardDoc.relatedLots }}"],
            {{ commaIf(awardDoc.title) }}
            {{ stringAttributeIf(name = "title", value = awardDoc.title) }}
            {{ commaIf(awardDoc.description) }}
            {{ stringAttributeIf(name = "description", value = awardDoc.description) }}
          }
        {% endfor %}
      ]
    },
    "terms": [
      {% for term in context.terms %}
        {% if loop.index  > 0 %},{% endif %}
        {
          "id": "{{ term.id }}",
          "title": "{{ term.title }}",
          "description": "{{ term.description }}",
          "observations": [
            {% for observation in term.observations %}
              {% if loop.index  > 0 %},{% endif %}
              {
                "id": "{{ observation.id }}",
                "notes": "{{ observation.notes }}",
                "measure": "{{ observation.measure }}"
              }
            {% endfor %}
          ]
        }
      {% endfor %}
    ],
    "parties": {
      "buyer": {
        "id": "{{ context.buyer.id }}",
        "name": "{{ context.buyer.name }}"
        "address": {
          "location": {
            "country": {
              "id": "{{ context.buyer.address.country.id }}",
              "description": "{{ context.buyer.address.country.description }}"
            },
            "region": {
              "id": "{{ context.buyer.address.region.id }}",
              "description": "{{ context.buyer.address.region.description }}"
            },
            "locality": {
              "localityType": {% if context.buyer.address.locality.scheme == "CUATM" %}"{{ translate("cn", "section2.lots.performance.address.location.locality.localityType.enum[0]") }}"{% else %}"{{ translate("cn", "section2.lots.performance.address.location.locality.localityType.enum[1]") }}"{% endif %},
              {% if context.buyer.address.locality.scheme == "CUATM" %}"directory"{% else %}"manual"{% endif %}: {
                "locality": {
                "scheme": "{{ context.buyer.address.locality.scheme }}",
                "id": "{{ context.buyer.address.locality.id }}",
                "description": "{{ context.buyer.address.locality.description }}"
                }
              }
            }
          },
          "postalAddress": {
          "streetAddress": "{{ context.buyer.address.streetAddress }}"
          {{ commaIf(context.buyer.address.postalCode) }}
          {{ stringAttributeIf(name = "postalCode", value = context.buyer.address.postalCode) }}
          }
        },
        "identifier": {
          "scheme": "{{ context.buyer.identifier.scheme }}",
          "id": "{{ context.buyer.identifier.id }}",
          "legalName": "{{ context.buyer.identifier.legalName }}"
          {{ commaIf(context.buyer.identifier.uri) }}
          {{ stringAttributeIf(name = "uri", value = context.buyer.identifier.uri) }}
        },
        {% if context.buyer.additionalIdentifiers != null %}
          "additionalIdentifiers": [
            {% for buyerAdditionalIdentifier in context.buyer.additionalIdentifiers %}
              {% if loop.index  > 0 %},{% endif %}
              {
                "scheme": "{{ buyerAdditionalIdentifier.scheme }}",
                "id": "{{ buyerAdditionalIdentifier.id }}",
                "legalName": "{{ buyerAdditionalIdentifier.legalName }}"
                {{ commaIf(buyerAdditionalIdentifier.uri) }}
                {{ stringAttributeIf(name = "uri", value = buyerAdditionalIdentifier.uri) }}
              }
            {% endfor %}
          ],
        {% endif %}
        "contactPoint": {
          "person": {
            "name": "{{ context.buyer.contactPoint.name }}"
            {{ commaIf(context.buyer.contactPoint.url) }}
            {{ stringAttributeIf(name = "url", value = context.buyer.contactPoint.url) }}
          },
          "contacts": {
            "telephone": "{{ context.buyer.contactPoint.telephone }}",
            "email": "{{ context.buyer.contactPoint.email }}"
            {{ commaIf(context.buyer.contactPoint.faxNumber) }}
            {{ stringAttributeIf(name = "faxNumber", value = context.buyer.contactPoint.faxNumber) }}
          }
        },
        "details": {
          {{ stringAttributeIf(name = "url", value = context.buyer.details.typeOfBuyer ) }}
          {{ commaIf(context.buyer.details.mainGeneralActivity) }}
          {{ stringAttributeIf(name = "url", value = context.buyer.details.mainGeneralActivity ) }}
          {{ commaIf(context.buyer.details.mainSectoralActivity) }}
          {{ stringAttributeIf(name = "url", value = context.buyer.details.mainSectoralActivity ) }}
          {% if context.buyer.details.bankAccounts != null %}
            "bankAccounts": [
              {% for buyerBankAccount in context.buyer.details.bankAccounts %}
                {% if loop.index  > 0 %},{% endif %}
                {
                  "bankName": "{{ buyerBankAccount.bankName }}",
                  "description": "{{ buyerBankAccount.description }}",
                  "address": {
                    "location": {
                      "country": {
                        "id": "{{ buyerBankAccount.address.country.id }}",
                        "description": "{{ buyerBankAccount.address.country.description }}"
                      },
                      "region": {
                        "id": "{{ buyerBankAccount.address.region.id }}",
                        "description": "{{ buyerBankAccount.address.region.description }}"
                      },
                      "locality": {
                        "localityType": {% if buyerBankAccount.address.locality.scheme == "CUATM" %}"{{ translate("cn", "section2.lots.performance.address.location.locality.localityType.enum[0]") }}"{% else %}"{{ translate("cn", "section2.lots.performance.address.location.locality.localityType.enum[1]") }}"{% endif %},
                        {% if buyerBankAccount.address.locality.scheme == "CUATM" %}"directory"{% else %}"manual"{% endif %}: {
                          "locality": {
                            "scheme": "{{ buyerBankAccount.address.locality.scheme }}",
                            "id": "{{ buyerBankAccount.address.locality.id }}",
                            "description": "{{ buyerBankAccount.address.locality.description }}"
                          }
                        }
                      }
                    },
                    "postalAddress": {
                      "streetAddress": "{{buyerBankAccount.address.streetAddress }}"
                      {{ commaIf(buyerBankAccount.address.postalCode) }}
                      {{ stringAttributeIf(name = "postalCode", value = buyerBankAccount.address.postalCode) }}
                    }
                  },
                  "identifier": {
                    "scheme": "{{ buyerBankAccount.identifier.scheme }}",
                    "id": "{{ buyerBankAccount.identifier.id }}"
                  },
                  "accountIdentification": {
                    "scheme": "{{ buyerBankAccount.accountIdentification.scheme }}",
                    "id": "{{ buyerBankAccount.accountIdentification.id }}"
                  },
                  {% if buyerBankAccount.additionalAccountIdentifiers != null %}
                    "additionalAccountIdentifiers": [
                      {% for buyerAdditionalAccountIdentifier in buyerBankAccount.additionalAccountIdentifiers %}
                        {% if loop.index  > 0 %},{% endif %}
                        {
                          "scheme": "{{ buyerAdditionalAccountIdentifier.scheme }}",
                          "id": "{{ buyerAdditionalAccountIdentifier.id }}"
                        }
                      {% endfor %}
                    ],
                  {% endif %}
                }
              {% endfor %}
            ],
          {% endif %}
          {% if context.buyer.details.legalForm.scheme != null %}
            "legalForm": {
              "scheme": "{{ context.buyer.details.legalForm.scheme }}"
              "id": "{{ context.buyer.details.legalForm.id }}"
              "description": "{{ context.buyer.details.legalForm.description }}"
              "uri": "{{ context.buyer.details.legalForm.uri }}"
            }
          {% endif %}
        }
        {% if context.buyer.persones != null %}
          "persones": [
            {% for buyerPersone in context.buyer.persones %}
              {% if loop.index  > 0 %},{% endif %}
              {
                "title": "{{ buyerPersone.title }}",
                "name": "{{ buyerPersone.name }}",
                "identifier": {
                  "scheme": "{{ buyerPersone.identifier.scheme }}",
                  "id": "{{ buyerPersone.identifier.id }}",
                  "uri": "{{ buyerPersone.identifier.uri }}"
                },
                "businessFunctions": [
                  {% for buyerBusinessFunction in buyerPersone.businessFunctions %}
                    {% if loop.index  > 0 %},{% endif %}
                    {
                      "id": "{{ buyerBusinessFunction.id }}",
                      "type": "{{ buyerBusinessFunction.type }}",
                      "jobTitle": "{{ buyerBusinessFunction.jobTitle }}",
                      "startDate": "{{ buyerBusinessFunction.startDate }}",
                      "documents": [
                        {% for buyerBusinessFunctionDocument in buyerBusinessFunction.documents %}
                          {% if loop.index  > 0 %},{% endif %}
                          {
                            "documentId": "{{ buyerBusinessFunctionDocument.id }}",
                            "documentType": "{{ buyerBusinessFunctionDocument.type }}",
                            {{ commaIf(buyerBusinessFunctionDocument.title) }}
                            {{ stringAttributeIf(name = "title", value = buyerBusinessFunctionDocument.title) }}
                            {{ commaIf(buyerBusinessFunctionDocument.description) }}
                            {{ stringAttributeIf(name = "description", value = buyerBusinessFunctionDocument.description) }}
                          }
                        {% endfor %}
                      ]
                    }
                  {% endfor %}
                ]
              }
            {% endfor %}
          ]
        {% endif %}
      },
      "suppliers": [
        {% for  %}
      ]
    }
  }
}
      "fsBudget": {
        "title": "{{ translate("fsBudget.title") }}",
        "type": "object",
        "properties": {
          "rationale": {
            "title": "{{ translate("fsBudget.rationale.title") }}",
            "type": "string",
            "ocds": "planning::object/rationale::string"
          },
          "budgetDetails": {
            "title": "{{ translate("fsBudget.budgetDetails.title") }}",
            "type": "object",
            "properties": {
              "description": {
                "title": "{{ translate("fsBudget.budgetDetails.description.title") }}",
                "description": "{{ translate("fsBudget.budgetDetails.description.description") }}",
                "type": "string",
                "ocds": "planning::object/budget::object/description::string"
              },
              "id": {
                "title": "{{ translate("fsBudget.budgetDetails.id.title") }}",
                "description": "{{ translate("fsBudget.budgetDetails.id.description") }}",
                "type": "string",
                "ocds": "planning::object/budget::object/id::string"
              }
            }
          },
          "budgetAmount": {
            "title": "{{ translate("fsBudget.budgetAmount.title") }}",
            "type": "object",
            "required": [
              "amount",
              "currency"
            ],
            "properties": {
              "amount": {
                "title": "{{ translate("fsBudget.budgetAmount.amount.title") }}",
                "type": "number",
                "minimum": 0.00001,
                "ocds": "planning::object/budget::object/amount::object/amount::number"
              },
              "currency": {
                "title": "{{ translate("fsBudget.budgetAmount.currency.title") }}",
                "type": "string",
                "source": "{{ context.uris.currency }}",
                {% if context.budget.amount is not null %}
                  "readOnly": true,
                {% endif %}
                "ocds": "planning::object/budget::object/amount::object/currency::string"
              }
            }
          },
          "budgetPeriod": {
            "title": "{{ translate("fsBudget.budgetPeriod.title") }}",

                    ],
                    "properties": {
                      "isEUfunded": {
                        "title": "{{ translate("fsBudget.budgetProject.projectDetails.EUfunded.isEUfunded.title") }}",
                        "type": "boolean",
                        "enum": [
                          true,
                          false
                        ],
                        "enumNames": [

            "required": [
              "startDate",
              "endDate"
            ],
            "properties": {
              "startDate": {
                "title": "{{ translate("fsBudget.budgetPeriod.startDate.title") }}",
                "type": "string",
                "ocds": "planning::object/budget::object/period::object/startDate::string"
              },
              "endDate": {
                "title": "{{ translate("fsBudget.budgetPeriod.endDate.title") }}",
                "type": "string",
                "ocds": "planning::object/budget::object/period::object/endDate::string"
              }

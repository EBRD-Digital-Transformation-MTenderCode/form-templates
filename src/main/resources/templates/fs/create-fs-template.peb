                        ],
                        "enumDesc": [
                          "{{ translate("fsBudget.budgetProject.projectDetails.EUfunded.isEUfunded.enumDesc[0]") }}",
                          "{{ translate("fsBudget.budgetProject.projectDetails.EUfunded.isEUfunded.enumDesc[1]") }}"
                        ],
                        "readOnly": true,
                        "ocds": "planning::object/budget::object/isEuropeanUnionFunded::boolean"
